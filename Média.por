/*Crie um algoritmo que leia dois números e calcule a média.*/


programa {
  funcao inicio() {

inteiro numero1,numero2,total

escreva("Escolha 1 numero? ")
leia(numero1)

escreva("Escolha o 2 numero? ")
leia(numero2)

total = (numero1 + numero2) / 2

escreva("O total dos dois numeros é: " + total)




  }
}
